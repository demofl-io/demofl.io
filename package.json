{
  "type": "module",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@vitejs/plugin-legacy": "^6.1.1",
    "cssnano": "^7.0.7",
    "daisyui": "^5.0.43",
    "postcss": "^8.5.5",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.10",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.0.2",
    "vite-plugin-web-extension": "^4.4.4"
  },
  "name": "demofl.io",
  "version": "1.0.0",
  "description": "This extension aims to make life as an SE (Sales / Solutions Engineer) easier but also to make demos more attractive to prospects.",
  "main": "tailwind.config.js",
  "scripts": {
    "dev": "vite build --watch",
    "build": "npm run clean && npm run build:extension-files && npm run build:regular-files && npm run build:css",
    "clean": "rm -rf dist",
    "build:extension-files": "npm run build:content && npm run build:background && npm run build:auth-content && npm run build:demofliocloud && npm run build:popup && npm run build:editor && npm run build:processor && npm run build:config && npm run build:overview && npm run build:personas",
    "build:content": "vite build --config vite.extension.config.js --mode content",
    "build:background": "vite build --config vite.extension.config.js --mode background", 
    "build:auth-content": "vite build --config vite.extension.config.js --mode auth-content",
    "build:demofliocloud": "vite build --config vite.extension.config.js --mode demofliocloud",
    "build:popup": "vite build --config vite.extension.config.js --mode popup",
    "build:editor": "vite build --config vite.extension.config.js --mode editor",
    "build:processor": "vite build --config vite.extension.config.js --mode processor",
    "build:config": "vite build --config vite.extension.config.js --mode config",
    "build:overview": "vite build --config vite.extension.config.js --mode overview",
    "build:personas": "vite build --config vite.extension.config.js --mode personas",
    "build:regular-files": "vite build --mode static-only",
    "build:css": "postcss css/input.css -o dist/output.css",
    "preview": "vite preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "extpay": "^3.1.1"
  }
}
